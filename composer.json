{
    "name": "andre/sysadmin",
    "description": "Um sistema para administração completa da empresa urnas mart",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "jms/serializer": "^2.2",
        "knplabs/knp-paginator-bundle": "^3.0",
        "respect/validation": "^1.1",
        "sensio/framework-extra-bundle": "^5.2",
        "spatie/browsershot": "^3.30",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^4.0",
        "twig/extensions": "^1.5",
        "vich/uploader-bundle": "^1.9",
        "wgenial/numeroporextenso": "^2.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "nunomaduro/phpinsights": "^1.5",
        "phpstan/phpstan": "^0.10.5",
        "phpstan/phpstan-doctrine": "^0.10.0",
        "phpstan/phpstan-symfony": "^0.10.2",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug": "^4.0",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/profiler-pack": "^1.0",
        "symfony/templating": "^4.0",
        "symfony/var-dumper": "^4.0",
        "symplify/easy-coding-standard": "^6.0"
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
	"ecs" : "php vendor/bin/ecs check src",
	"ecsfix": "php vendor/bin/ecs check src --fix",
	"pins": "php vendor/bin/phpinsights"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "repositories": {
        "phpunit-bridge": {
            "type": "path",
            "url": "C:\\Users\\yasmi\\projects\\sysadmin\\vendor\\symfony\\phpunit-bridge"
        }
    }
}
