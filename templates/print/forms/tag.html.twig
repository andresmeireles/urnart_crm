<body>
<div class="col"> {# full-page #}
    {% for data in prod %}
        {% for tag in range(1, data.amount) %}
        {% set logo =  ('/pdf' in app.request.server.get('REQUEST_URI')) ? '../'~logo : asset(logo) %}
        <div class="row py-2">

                <div class="col-3">
                    <img class="col" src="{{ logo }}">
                </div>

                <div class="col">
                    {% if data.city == '' %}
                        {% set fontsize = 'display-1' %}
                    {% elseif data | length > 27 %}
                        {% set fontsize = 'display-1' %}
                    {% else %}
                        {% set fontsize = 'display-4' %}
                    {% endif %}
                    <span class="{{ fontsize }}">{{ data.name }}</span><br>
                    <span class="h1">{{ data.city }}</span>
                </div>

                <div class="col-3 text-center">
                    <span class="display-4">{{ data.check != 0 ? (tag/2) | round(0, 'ceil') : tag }}</span><br>
                    <span class="h2">VOL. {{ data.check != 0 ? (data.amount/2) | round(0, 'ceil') : data.amount }}</span>
                </div>

        </div>
        <hr>
        {% endfor %}
    {% endfor %}
</div> {# end of full-page #}
</body>
</html>